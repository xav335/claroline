{% for note in pager.currentPageResults %}
    <div id="socialmedia-note-item-{{ note.id }}" class="list-group-item">
        <button type="button" data-item-id="{{ note.id }}" class="close remove-sociamedia-note-item"><span aria-hidden="true">&times;</span></button>
        <span>{{ note.text }}</span><br/>
        <span><small class="small">{{ note.creationDate|date("date_format"|trans({}, "icap_socialmedia")) }}</small></span>
    </div>
{% endfor %}
{% if pager.hasNextPage %}
    <div class="list-group-item"><a class="jscroll-note-list-next" href="{{ path("icap_socialmedia_notelist", {"resourceId":resourceId, "page":pager.nextPage}) }}"><i class="fa fa-long-arrow-down"></i> {{ "load_next_notes"|trans({}, 'icap_socialmedia') }}</a></div>
{% endif %}