{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "type": {
      "enum": ["application/x.match+json"]
    },
    "data": {
      "type": "array",
      "uniqueItems": true,
      "items": {
        "type": "object",
        "properties": {
          "firstId": {
            "type": "string"
          },
          "secondId": {
            "type": "string"
          }
        },
        "required": ["firstId", "secondId"]
      }
    }
  }
}
